{
  "name": "networkview-is",
  "version": "1.0.0",
  "description": "Информационная система NetworkView IS с интерактивными виджетами для управления документацией ИБ и диаграммами",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"nodemon server/index.js\" \"live-server --port=3001 --host=localhost --open=./\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "live-server --port=3001 --host=localhost --open=./"
  },
  "keywords": ["information-security", "network-diagrams", "drawio", "document-management"],
  "author": "NetworkView IS Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "xml2js": "^0.5.0",
    "marked": "^5.1.1",
    "uuid": "^9.0.0",
    "node-cron": "^3.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "live-server": "^1.2.2"
  }
} 